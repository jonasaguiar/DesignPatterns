
public class StringTests {

	
	
	public static void main(String[] args) {
		
		
		String palavra = "abcdefghij";
		String numero = "123456789";
		String palavra2 = "ABCDEFGHIJ";
		int numeromesmo = 10;
		
		int posição = 1;
		
		String letra = "w";
		
		if(palavra.contains(letra)) {
			System.out.println("A palavra " + palavra + " contém a letra " + letra + "!");
		}else {
			System.out.println("A palavra " + palavra + " não contém a letra " + letra + "!");
		}
		
		System.out.println("O numero na posição " + posição + " é " + palavra.charAt(posição));
		
		if(palavra.compareToIgnoreCase(palavra2) == 0) {
			System.out.println("As palavras" + palavra + " e " + palavra2 + " são iguais!");
		}else {
			System.out.println("As palavras" + palavra + " e " + palavra2 + " são diferentes!");
		}
		
		
		
	}
	
}
