
public class Porta {

	private boolean estaAberta;
	private String cor;
	private int dimensaoX;
	private int dimensaoY;
	private int dimens찾oZ;
	
	public Porta(String cor, int dimensaoX, int dimensaoY, int dimensaoZ) {
		this.cor = cor;
		this.dimensaoX = dimensaoX;
		this.dimensaoY = dimensaoY;
		this.dimens찾oZ = dimensaoZ;
		this.estaAberta = true;
	}
	
	public void abre() {
		this.estaAberta = true;
	}
	
	public void fecha() {
		this.estaAberta = false;
	}
	
	public void pinta(String cor) {
		this.cor = cor;
	}
	
	public boolean estaAberta() {
		return this.estaAberta;
		}
	
	public static void main(String[] args) {
		Porta porta = new Porta("Branca", 10, 20, 10);
		
		System.out.println();
		
		
		
	}
	
	@Override
	public String toString() {
		String mensagem = "A porta tem a cor " + cor + ", com dimens천es X, Y e Z sendo respectivamente: " + dimensaoX + ", " + dimensaoY + ", " + dimens찾oZ "."; 
		return ;
	}
	
}
	
	
	
	

