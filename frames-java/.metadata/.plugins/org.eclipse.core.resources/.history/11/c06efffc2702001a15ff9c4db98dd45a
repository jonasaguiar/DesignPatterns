
public class StringTests {

	
	
	public static void main(String[] args) {
		
		
		String palavra = "abcdefghij";
		String numero = "123456789";
		String palavra2 = "ABCDEFGHIJ";
		
		System.out.println(palavra.codePointCount(0, 2));
		System.out.println(palavra.charAt(1));
		System.out.println(palavra.compareTo(palavra2));
		
		
		
	}
	
}
